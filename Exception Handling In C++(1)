#include <iostream>
using namespace std;

int main()
{

 try{
    int num1,num2;
 
    cout << "Enter the 1st Number:" << endl;
    cin >> num1;

     cout << "Enter the 2nd Number:" << endl;
     cin >> num2;
     
     if(num2 == 0)
     {
         throw "Divided by Zero is not possible";
     }
 
     double result = (double) num1 / num2;
 
     cout <<"Result :" << result << endl;
     
 }
 
 catch(const char* x)
 {
     cout << x << endl;
 }

    
    
    
    
    
    
    
    return 0;
}
