#include <iostream>
using namespace std;

template <typename T>
T add(T a, T b)
{
 
 return a + b;
 
}

template <typename T>
T subtract(T a, T b)
{
    return a - b;
}

template <typename T>
T multiplication(T a, T b)
{
    return a*b;
}

template <typename T>
T Division(T a, T b)
{
   if(b == 0)
   {
       
   }
    return a / b;
}
int main() {
    
 
 cout << add(5,7) << endl;
 cout << add(5.5,7.5) << endl;
 
 
 cout << subtract(7,5) << endl;
 cout << subtract(7.5,5.5) << endl;
 
 cout << multiplication(5,3) << endl;
 cout << multiplication(5.5,2.2) << endl;
 
 
 
 
 
 
    return 0;
}
